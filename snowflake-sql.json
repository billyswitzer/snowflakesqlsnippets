{
	// Place your snippets for snowflake-sql here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"ALTER FUNCTION fragment": {
		"prefix": "af",
		"body": [
			"ALTER FUNCTION "
		]
	},
	"ALTER TABLE fragment": {
		"prefix": "at",
		"body": [
			"ALTER TABLE "
		]
	},
	"Add column": {
		"prefix": "ata",
		"body": [
			"ALTER TABLE ${TableName}\nADD ${ColumnName}"
		]
	},
	"Modify column": {
		"prefix": "atac",
		"body": [
			"ALTER TABLE ${TableName}\nALTER COLUMN "
		]
	},
	"Drop column": {
		"prefix": "atd",
		"body": [
			"ALTER TABLE ${TableName}\nDROP COLUMN "
		]
	},
	"Drop constraint": {
		"prefix": "atdc",
		"body": [
			"ALTER TABLE ${TableName}\nDROP CONSTRAINT "
		]
	},
	"BEGIN...END block": {
		"prefix": "be",
		"body": [
			"BEGIN\n\t$0\nEND;"
		]
	},
	"Create function": {
		"prefix": "cf",
		"body": [
			"CREATE FUNCTION ${1:FunctionName} ($2) RETURNS $3 AS\n"
		]
	},
	"Create or replace function": {
		"prefix": "cf",
		"body": [
			"CREATE OR REPLACE FUNCTION ${1:FunctionName} ($2) RETURNS $3 AS\n"
		]
	},
	"CROSS JOIN fragment": {
